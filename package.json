{
  "name": "snackbar-messages-nuxt",
  "version": "1.0.0",
  "description": "snackbar messages module for nuxt",
  "repository": "Felix-xilef/nuxt-snackbar-messages",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.1592"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.3",
    "@nuxt/eslint-config": "^0.7.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.14.1592",
    "@nuxt/test-utils": "^3.14.4",
    "@pinia/nuxt": "0.9.0",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.16.0",
    "nuxt": "^3.14.1592",
    "pinia": "^2.3.0",
    "typescript": "latest",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10",
    "vuetify-nuxt-module": "0.18.3"
  },
  "packageManager": "yarn@4.5.1+sha512.341db9396b6e289fecc30cd7ab3af65060e05ebff4b3b47547b278b9e67b08f485ecd8c79006b405446262142c7a38154445ef7f17c1d5d1de7d90bf9ce7054d"
}
